{
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 4,
    "semi": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-tailwindcss"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
